<!DOCTYPE html>
<html lang="en">

<head>
    <meta charset="UTF-8">
    <meta http-equiv="X-UA-Compatible" content="IE=edge">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <script src="https://ajax.googleapis.com/ajax/libs/jquery/3.5.1/jquery.min.js"></script>
    <script src="https://maxcdn.bootstrapcdn.com/bootstrap/3.4.1/js/bootstrap.min.js"></script>
    <script src="https://cdnjs.cloudflare.com/ajax/libs/popper.js/1.16.0/umd/popper.min.js"></script>
    <link rel="stylesheet" href="https://cdn.jsdelivr.net/npm/bootstrap@4.5.3/dist/css/bootstrap.min.css" integrity="sha384-TX8t27EcRE3e/ihU7zmQxVncDAy5uIKz4rEkgIXeMed4M0jlfIDPvg6uqKI2xXr2" crossorigin="anonymous">

    <title>Add Super Admin Page</title>
</head>

<body>

    <div class="container text-center main-div mt-5">
        <div class="row justify-content-center">
            <div class="col-lg-4 col-sm-6 col-md-6">

                <form class="form-div" id="form">

                    <h2 class="mb-3">Add Super Admin</h2>
                    <div class="form-group">
                        <input required value="" type="text" name="name" id="name" class="form-control" placeholder="Name">
                    </div>
                    <div class="form-name"></div>

                    <div class="form-group">
                        <input required value="" type="text" name="email" id="email" class="form-control" placeholder="Email Id">
                    </div>
                    <div class="form-email"></div>

                    <div class="form-group">
                        <input required value="" type="number" name="contactNo" id="contactNo" class="form-control" placeholder="Contact No">
                    </div>
                    <div class="form-contact"></div>

                    <div class="form-group">
                        <input required value="" type="text" name="gender" id="gender" class="form-control" placeholder="Gender">
                    </div>

                    <div class="form-group">
                        <input required value="" type="text" name="subject" id="subject" class="form-control" placeholder="Subject">
                    </div>

                    <div class="form-group">
                        <input required value="" type="password" name="password" id="password" class="form-control" placeholder="Password">
                    </div>
                    <div class="form-password"></div>

                    <div class="form-group">
                        <input required value="" type="password" name="confirm_password" id="confirm-password" class="form-control" placeholder="Confirm
                                Password">
                    </div>
                    <div class="form-cpassword"></div>

                    <div class="form-group mt-4">
                        <button class="btn btn-primary btn-block" type="submit" value="Submit" id="submit-btn">Submit</button>
                    </div>

                </form>
            </div>
        </div>
    </div>

    <!-- <script src="./../../public/registerjs.js"></script> -->
    <script src="https://cdn.jsdelivr.net/npm/popper.js@1.16.0/dist/umd/popper.min.js" integrity="sha384-Q6E9RHvbIyZFJoft+2mJbHaEWldlvI9IOYy5n3zV9zzTtmI3UksdQRVvoxMfooAo" crossorigin="anonymous"></script>
    <script src="https://stackpath.bootstrapcdn.com/bootstrap/5.0.0-alpha1/js/bootstrap.min.js" integrity="sha384-oesi62hOLfzrys4LxRF63OJCXdXDipiYWBnvTl9Y9/TRlw5xlKIEHpNyvvDShgf/" crossorigin="anonymous"></script>

    <!-- Script for Registration -->
    <script src="script/addNormalAdmin.js"></script>

</body>

</html>